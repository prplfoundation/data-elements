
config wifi-device 'radio0'
	option type 'mac80211'
	option channel '36'
	option hwmode '11a'
	option path 'pci0000:01/0000:01:00.0'
	option disabled '0'
	option country 'US'
	option htmode 'VHT80'
	option txpower '23'

config wifi-iface
	option device 'radio0'
	option network 'lan'
	option mode 'ap'
	option encryption 'psk2'
	option key 'Wccf.Ap.Pw'
	option ssid 'WccfOpenWrt-5'

config wifi-device 'radio1'
	option type 'mac80211'
	option channel '11'
	option hwmode '11g'
	option path 'platform/qca955x_wmac'
	option htmode 'HT20'
	option disabled '0'
	option country 'US'
	option txpower '24'

config wifi-iface
	option device 'radio1'
	option network 'lan'
	option mode 'ap'
	option encryption 'psk2'
	option key 'Wccf.Ap.Pw'
	option ssid 'WccfOpenWrt-2.4'

