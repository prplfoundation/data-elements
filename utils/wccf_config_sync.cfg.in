# Configuration for OpenWrt WCCF configuration synchronization system
#
# Update these values consistent with installed wccf_config_sync.sh program
#
# These values likely DO require change

export WCCF_CONFIG_SYNC_HOST=10.10.10.21
export WCCF_CONFIG_SYNC_USER=wccfadm

# These values likely DO NOT require change

export WCCF_CONFIG_REMOTE_PATH=config_sync
export WCCF_ADMIN_PRIVATE_KEY_NAME=/etc/config/wccf_openwrt_privkey

